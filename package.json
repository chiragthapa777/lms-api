{
  "name": "course-cloud",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run build && node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:migration": "pnpm run migration:generate -- src/database/migrations/pending-migrations",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "pnpm migration:run && jest --config ./test/jest-e2e.json",
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run migration:run && npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "migration:prod": "npx typeorm --dataSource=dist/src/database/data-source.js migration:run",
    "docker:push": "docker build -t chiragthapa777/coursecloud-recommender:v1 . && docker push chiragthapa777/coursecloud-recommender:v1",
    "ls": "ls"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.513.0",
    "@aws-sdk/s3-presigned-post": "3.513.0",
    "@aws-sdk/s3-request-presigner": "3.513.0",
    "@faker-js/faker": "^8.4.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "10.0.1",
    "@sentry/node": "^8.13.0",
    "@sentry/profiling-node": "^8.13.0",
    "axios": "^1.6.7",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.2",
    "bull": "^4.12.2",
    "bytes": "^3.1.2",
    "cache-manager": "^5.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "^16.3.1",
    "file-type": "^16.5.3",
    "helmet": "^7.1.0",
    "joi": "^17.12.1",
    "moment": "^2.30.1",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "nest-winston": "^1.9.4",
    "nodemailer": "^6.9.9",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pdfmake": "^0.2.10",
    "pg": "8.11.3",
    "piscina": "^4.6.1",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.19",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/ms": "0.7.34",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": [
      "../test/setup.ts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
