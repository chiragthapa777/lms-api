{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "../swagger-theme/*",
        "watchAssets": true,
        "outDir": "dist/swagger-theme"
      },
      {
        "include": "../fonts/*",
        "watchAssets": true,
        "outDir": "dist/fonts"
      }
    ]
  }
}
